{
  "designSystem": {
    "name": "[PROJECT_NAME]",
    "version": "[VERSION]",
    "framework": "[CSS_FRAMEWORK]",
    "theme": {
      "colors": {
        "primary": {
          "50": "[PRIMARY_50]",
          "100": "[PRIMARY_100]",
          "200": "[PRIMARY_200]",
          "300": "[PRIMARY_300]",
          "400": "[PRIMARY_400]",
          "500": "[PRIMARY_500]",
          "600": "[PRIMARY_600]",
          "700": "[PRIMARY_700]",
          "800": "[PRIMARY_800]",
          "900": "[PRIMARY_900]"
        },
        "secondary": {
          "50": "[SECONDARY_50]",
          "100": "[SECONDARY_100]",
          "200": "[SECONDARY_200]",
          "300": "[SECONDARY_300]",
          "400": "[SECONDARY_400]",
          "500": "[SECONDARY_500]",
          "600": "[SECONDARY_600]",
          "700": "[SECONDARY_700]",
          "800": "[SECONDARY_800]",
          "900": "[SECONDARY_900]"
        },
        "success": {
          "light": "[SUCCESS_LIGHT]",
          "default": "[SUCCESS_DEFAULT]",
          "dark": "[SUCCESS_DARK]",
          "text": "[SUCCESS_TEXT]"
        },
        "danger": {
          "light": "[DANGER_LIGHT]",
          "default": "[DANGER_DEFAULT]",
          "dark": "[DANGER_DARK]",
          "text": "[DANGER_TEXT]"
        },
        "warning": {
          "light": "[WARNING_LIGHT]",
          "default": "[WARNING_DEFAULT]",
          "dark": "[WARNING_DARK]",
          "text": "[WARNING_TEXT]"
        },
        "info": {
          "light": "[INFO_LIGHT]",
          "default": "[INFO_DEFAULT]",
          "dark": "[INFO_DARK]",
          "text": "[INFO_TEXT]"
        },
        "neutral": {
          "50": "[NEUTRAL_50]",
          "100": "[NEUTRAL_100]",
          "200": "[NEUTRAL_200]",
          "300": "[NEUTRAL_300]",
          "400": "[NEUTRAL_400]",
          "500": "[NEUTRAL_500]",
          "600": "[NEUTRAL_600]",
          "700": "[NEUTRAL_700]",
          "800": "[NEUTRAL_800]",
          "900": "[NEUTRAL_900]"
        },
        "background": {
          "primary": "[BG_PRIMARY]",
          "secondary": "[BG_SECONDARY]",
          "tertiary": "[BG_TERTIARY]",
          "card": "[BG_CARD]",
          "overlay": "[BG_OVERLAY]"
        },
        "text": {
          "primary": "[TEXT_PRIMARY]",
          "secondary": "[TEXT_SECONDARY]",
          "tertiary": "[TEXT_TERTIARY]",
          "inverse": "[TEXT_INVERSE]"
        },
        "border": {
          "light": "[BORDER_LIGHT]",
          "default": "[BORDER_DEFAULT]",
          "dark": "[BORDER_DARK]"
        },
        "indicators": {
          "positive": "#10b981",
          "negative": "#ef4444",
          "neutral": "#6b7280"
        }
      },
      "typography": {
        "fontFamily": {
          "sans": "[FONT_SANS]",
          "serif": "[FONT_SERIF]",
          "mono": "[FONT_MONO]",
          "display": "[FONT_DISPLAY]"
        },
        "fontSize": {
          "xs": "[FONT_SIZE_XS]",
          "sm": "[FONT_SIZE_SM]",
          "base": "[FONT_SIZE_BASE]",
          "lg": "[FONT_SIZE_LG]",
          "xl": "[FONT_SIZE_XL]",
          "2xl": "[FONT_SIZE_2XL]",
          "3xl": "[FONT_SIZE_3XL]",
          "4xl": "[FONT_SIZE_4XL]",
          "5xl": "[FONT_SIZE_5XL]",
          "6xl": "[FONT_SIZE_6XL]"
        },
        "fontWeight": {
          "light": "[FONT_WEIGHT_LIGHT]",
          "normal": "[FONT_WEIGHT_NORMAL]",
          "medium": "[FONT_WEIGHT_MEDIUM]",
          "semibold": "[FONT_WEIGHT_SEMIBOLD]",
          "bold": "[FONT_WEIGHT_BOLD]",
          "extrabold": "[FONT_WEIGHT_EXTRABOLD]"
        },
        "lineHeight": {
          "tight": "[LINE_HEIGHT_TIGHT]",
          "normal": "[LINE_HEIGHT_NORMAL]",
          "relaxed": "[LINE_HEIGHT_RELAXED]",
          "loose": "[LINE_HEIGHT_LOOSE]"
        }
      },
      "spacing": {
        "xs": "[SPACING_XS]",
        "sm": "[SPACING_SM]",
        "md": "[SPACING_MD]",
        "lg": "[SPACING_LG]",
        "xl": "[SPACING_XL]",
        "2xl": "[SPACING_2XL]",
        "3xl": "[SPACING_3XL]",
        "4xl": "[SPACING_4XL]"
      },
      "borderRadius": {
        "none": "[BORDER_RADIUS_NONE]",
        "sm": "[BORDER_RADIUS_SM]",
        "default": "[BORDER_RADIUS_DEFAULT]",
        "md": "[BORDER_RADIUS_MD]",
        "lg": "[BORDER_RADIUS_LG]",
        "xl": "[BORDER_RADIUS_XL]",
        "2xl": "[BORDER_RADIUS_2XL]",
        "full": "[BORDER_RADIUS_FULL]"
      },
      "shadows": {
        "sm": "[SHADOW_SM]",
        "default": "[SHADOW_DEFAULT]",
        "md": "[SHADOW_MD]",
        "lg": "[SHADOW_LG]",
        "xl": "[SHADOW_XL]",
        "2xl": "[SHADOW_2XL]",
        "inner": "[SHADOW_INNER]",
        "none": "[SHADOW_NONE]"
      },
      "transitions": {
        "fast": "[TRANSITION_FAST]",
        "normal": "[TRANSITION_NORMAL]",
        "slow": "[TRANSITION_SLOW]",
        "easing": {
          "default": "[EASING_DEFAULT]",
          "in": "[EASING_IN]",
          "out": "[EASING_OUT]",
          "inOut": "[EASING_IN_OUT]"
        }
      },
      "zIndex": {
        "dropdown": "[Z_DROPDOWN]",
        "sticky": "[Z_STICKY]",
        "fixed": "[Z_FIXED]",
        "modal": "[Z_MODAL]",
        "popover": "[Z_POPOVER]",
        "tooltip": "[Z_TOOLTIP]"
      }
    },
    "components": {
      "layout": {
        "sidebar": {
          "width": "16rem",
          "background": "neutral-900",
          "borderRight": "1px solid neutral-800",
          "padding": "spacing-md",
          "items": {
            "spacing": "spacing-xs",
            "padding": "spacing-sm spacing-md",
            "borderRadius": "borderRadius-md",
            "hover": {
              "background": "neutral-800",
              "transition": "transitions-normal"
            },
            "active": {
              "background": "neutral-800",
              "borderLeft": "3px solid primary-500"
            },
            "icon": {
              "size": "1.25rem",
              "marginRight": "spacing-sm"
            },
            "text": {
              "color": "neutral-300",
              "fontSize": "fontSize-sm",
              "fontWeight": "fontWeight-medium"
            }
          }
        },
        "header": {
          "height": "4rem",
          "background": "background-primary",
          "borderBottom": "1px solid neutral-200",
          "padding": "0 spacing-lg",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "space-between"
        },
        "mainContent": {
          "background": "background-secondary",
          "padding": "spacing-lg",
          "minHeight": "100vh"
        }
      },
      "card": {
        "base": {
          "background": "background-card",
          "borderRadius": "borderRadius-lg",
          "shadow": "shadows-default",
          "padding": "spacing-md",
          "border": "1px solid neutral-200"
        },
        "variants": {
          "elevated": {
            "shadow": "shadows-lg",
            "border": "none"
          },
          "flat": {
            "shadow": "none",
            "background": "background-secondary"
          },
          "outlined": {
            "shadow": "none",
            "border": "2px solid neutral-200"
          }
        },
        "header": {
          "marginBottom": "spacing-md",
          "display": "flex",
          "alignItems": "center",
          "justifyContent": "space-between"
        },
        "title": {
          "fontSize": "fontSize-lg",
          "fontWeight": "fontWeight-semibold",
          "color": "neutral-900"
        }
      },
      "button": {
        "base": {
          "padding": "0.5rem 1rem",
          "borderRadius": "borderRadius-md",
          "fontSize": "fontSize-sm",
          "fontWeight": "fontWeight-medium",
          "transition": "transitions-normal",
          "cursor": "pointer",
          "display": "inline-flex",
          "alignItems": "center",
          "justifyContent": "center",
          "gap": "spacing-xs"
        },
        "variants": {
          "primary": {
            "background": "primary-600",
            "color": "white",
            "hover": {
              "background": "primary-700"
            }
          },
          "secondary": {
            "background": "neutral-100",
            "color": "neutral-700",
            "border": "1px solid neutral-300",
            "hover": {
              "background": "neutral-200"
            }
          },
          "ghost": {
            "background": "transparent",
            "color": "neutral-600",
            "hover": {
              "background": "neutral-100"
            }
          },
          "danger": {
            "background": "danger-default",
            "color": "white",
            "hover": {
              "background": "danger-light"
            }
          }
        },
        "sizes": {
          "sm": {
            "padding": "0.25rem 0.75rem",
            "fontSize": "fontSize-xs"
          },
          "md": {
            "padding": "0.5rem 1rem",
            "fontSize": "fontSize-sm"
          },
          "lg": {
            "padding": "0.75rem 1.5rem",
            "fontSize": "fontSize-base"
          }
        }
      },
      "input": {
        "base": {
          "width": "100%",
          "padding": "0.5rem 0.75rem",
          "borderRadius": "borderRadius-md",
          "border": "1px solid neutral-300",
          "fontSize": "fontSize-sm",
          "background": "background-primary",
          "transition": "transitions-fast",
          "focus": {
            "outline": "none",
            "borderColor": "primary-500",
            "ring": "0 0 0 3px rgba(99, 102, 241, 0.1)"
          }
        },
        "label": {
          "fontSize": "fontSize-sm",
          "fontWeight": "fontWeight-medium",
          "color": "neutral-700",
          "marginBottom": "0.25rem",
          "display": "block"
        },
        "helperText": {
          "fontSize": "fontSize-xs",
          "color": "neutral-500",
          "marginTop": "0.25rem"
        },
        "error": {
          "borderColor": "danger-default",
          "focus": {
            "borderColor": "danger-default",
            "ring": "0 0 0 3px rgba(239, 68, 68, 0.1)"
          }
        }
      },
      "badge": {
        "base": {
          "display": "inline-flex",
          "alignItems": "center",
          "padding": "0.125rem 0.625rem",
          "borderRadius": "borderRadius-full",
          "fontSize": "fontSize-xs",
          "fontWeight": "fontWeight-medium"
        },
        "variants": {
          "success": {
            "background": "rgba(16, 185, 129, 0.1)",
            "color": "success-default"
          },
          "danger": {
            "background": "rgba(239, 68, 68, 0.1)",
            "color": "danger-default"
          },
          "warning": {
            "background": "rgba(245, 158, 11, 0.1)",
            "color": "yellow-600"
          },
          "info": {
            "background": "rgba(59, 130, 246, 0.1)",
            "color": "blue-600"
          },
          "neutral": {
            "background": "neutral-100",
            "color": "neutral-600"
          }
        }
      },
      "charts": {
        "container": {
          "padding": "spacing-sm",
          "height": "300px",
          "width": "100%"
        },
        "colors": {
          "primary": [
            "#6366f1",
            "#818cf8",
            "#a5b4fc",
            "#c7d2fe",
            "#e0e7ff"
          ],
          "semantic": [
            "#10b981",
            "#ef4444",
            "#f59e0b",
            "#3b82f6",
            "#8b5cf6"
          ],
          "gradient": {
            "from": "#6366f1",
            "to": "#a5b4fc"
          }
        },
        "grid": {
          "color": "neutral-200",
          "strokeDasharray": "3 3"
        },
        "axis": {
          "color": "neutral-400",
          "fontSize": "fontSize-xs",
          "fontWeight": "fontWeight-normal"
        },
        "tooltip": {
          "background": "neutral-900",
          "color": "white",
          "padding": "spacing-xs",
          "borderRadius": "borderRadius-md",
          "fontSize": "fontSize-xs",
          "shadow": "shadows-lg"
        },
        "types": {
          "line": {
            "strokeWidth": 2,
            "dot": {
              "size": 4,
              "strokeWidth": 2
            }
          },
          "bar": {
            "borderRadius": "borderRadius-sm",
            "gap": "20%"
          },
          "donut": {
            "innerRadius": "60%",
            "cornerRadius": 2
          },
          "gauge": {
            "innerRadius": "80%",
            "startAngle": -90,
            "endAngle": 90,
            "trackColor": "neutral-200",
            "labelFontSize": "fontSize-2xl",
            "sublabelFontSize": "fontSize-sm"
          }
        }
      },
      "dataTables": {
        "header": {
          "background": "neutral-50",
          "padding": "spacing-sm",
          "fontWeight": "fontWeight-semibold",
          "fontSize": "fontSize-sm",
          "color": "neutral-700",
          "borderBottom": "2px solid neutral-200"
        },
        "row": {
          "padding": "spacing-sm",
          "borderBottom": "1px solid neutral-100",
          "hover": {
            "background": "neutral-50"
          }
        },
        "cell": {
          "fontSize": "fontSize-sm",
          "color": "neutral-900"
        }
      },
      "metrics": {
        "container": {
          "display": "flex",
          "flexDirection": "column",
          "gap": "spacing-xs"
        },
        "value": {
          "fontSize": "fontSize-3xl",
          "fontWeight": "fontWeight-bold",
          "color": "neutral-900",
          "lineHeight": "lineHeight-tight"
        },
        "label": {
          "fontSize": "fontSize-sm",
          "color": "neutral-500",
          "fontWeight": "fontWeight-medium"
        },
        "change": {
          "fontSize": "fontSize-sm",
          "fontWeight": "fontWeight-medium",
          "display": "inline-flex",
          "alignItems": "center",
          "gap": "0.25rem",
          "positive": {
            "color": "success-default"
          },
          "negative": {
            "color": "danger-default"
          }
        }
      },
      "notifications": {
        "base": {
          "padding": "spacing-sm spacing-md",
          "borderRadius": "borderRadius-md",
          "display": "flex",
          "alignItems": "center",
          "gap": "spacing-sm",
          "fontSize": "fontSize-sm"
        },
        "variants": {
          "info": {
            "background": "blue-50",
            "color": "blue-900",
            "border": "1px solid blue-200"
          },
          "success": {
            "background": "green-50",
            "color": "green-900",
            "border": "1px solid green-200"
          },
          "warning": {
            "background": "yellow-50",
            "color": "yellow-900",
            "border": "1px solid yellow-200"
          },
          "error": {
            "background": "red-50",
            "color": "red-900",
            "border": "1px solid red-200"
          }
        }
      }
    },
    "patterns": {
      "dashboardGrid": {
        "display": "grid",
        "gridTemplateColumns": {
          "sm": "1fr",
          "md": "repeat(2, 1fr)",
          "lg": "repeat(3, 1fr)",
          "xl": "repeat(4, 1fr)"
        },
        "gap": "spacing-md"
      },
      "statCard": {
        "structure": {
          "icon": {
            "size": "2.5rem",
            "color": "primary-500",
            "background": "primary-50",
            "padding": "spacing-sm",
            "borderRadius": "borderRadius-lg"
          },
          "content": {
            "display": "flex",
            "flexDirection": "column",
            "gap": "spacing-xs"
          },
          "trend": {
            "display": "flex",
            "alignItems": "center",
            "gap": "spacing-xs",
            "fontSize": "fontSize-sm"
          }
        }
      },
      "listItem": {
        "display": "flex",
        "alignItems": "center",
        "justifyContent": "space-between",
        "padding": "spacing-sm 0",
        "borderBottom": "1px solid neutral-100",
        "hover": {
          "background": "neutral-50",
          "padding": "spacing-sm",
          "margin": "-spacing-sm",
          "borderRadius": "borderRadius-md"
        }
      },
      "emptyState": {
        "textAlign": "center",
        "padding": "spacing-2xl",
        "icon": {
          "size": "4rem",
          "color": "neutral-300",
          "marginBottom": "spacing-md"
        },
        "title": {
          "fontSize": "fontSize-lg",
          "fontWeight": "fontWeight-semibold",
          "color": "neutral-900",
          "marginBottom": "spacing-xs"
        },
        "description": {
          "fontSize": "fontSize-sm",
          "color": "neutral-500"
        }
      }
    },
    "animations": {
      "fadeIn": {
        "keyframes": {
          "from": {
            "opacity": 0
          },
          "to": {
            "opacity": 1
          }
        },
        "duration": "300ms",
        "easing": "ease-out"
      },
      "slideIn": {
        "keyframes": {
          "from": {
            "transform": "translateY(10px)",
            "opacity": 0
          },
          "to": {
            "transform": "translateY(0)",
            "opacity": 1
          }
        },
        "duration": "300ms",
        "easing": "ease-out"
      },
      "pulse": {
        "keyframes": {
          "0%, 100%": {
            "opacity": 1
          },
          "50%": {
            "opacity": 0.5
          }
        },
        "duration": "2s",
        "easing": "ease-in-out",
        "iterationCount": "infinite"
      }
    },
    "responsive": {
      "breakpoints": {
        "sm": "[BREAKPOINT_SM]",
        "md": "[BREAKPOINT_MD]",
        "lg": "[BREAKPOINT_LG]",
        "xl": "[BREAKPOINT_XL]",
        "2xl": "[BREAKPOINT_2XL]"
      },
      "containerMaxWidth": {
        "sm": "[CONTAINER_SM]",
        "md": "[CONTAINER_MD]",
        "lg": "[CONTAINER_LG]",
        "xl": "[CONTAINER_XL]",
        "2xl": "[CONTAINER_2XL]"
      }
    },
    "accessibility": {
      "focus": {
        "outline": "[FOCUS_OUTLINE]",
        "ring": "[FOCUS_RING]",
        "offset": "[FOCUS_OFFSET]"
      },
      "contrast": {
        "minimum": "[CONTRAST_MINIMUM]",
        "enhanced": "[CONTRAST_ENHANCED]"
      },
      "reducedMotion": {
        "prefersReducedMotion": "[REDUCED_MOTION_PREFERENCE]"
      }
    }
  }
}